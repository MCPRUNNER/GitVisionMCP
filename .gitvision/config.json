{
    "Project": {
        "Name": "GitVisionMCP",
        "RelativePath": "GitVisionMCP.csproj",
        "Version": "1.0.9.0"
    },
    "Settings": {
        "maxCommits": 200,
        "maxFiles": 500,
        "maxFileSize": 1048576
    },
    "Git": {
        "Release": "origin/GitVisionMCP-Release-1.0.9",
        "Master": "origin/master"
    },
     
    "Kestrel": {
        "Endpoints": {
            "Http": {
                "Url": "http://localhost:5000/mcp"
            },
            "Https": {
                "Url": "https://localhost:5001/mcp"
            }
        },
        "Limits": {
            "MaxRequestBodySize": 10485760,
            "MaxConcurrentConnections": 100,
            "RequestHeadersTimeout": "00:01:00"
        }
    }
}